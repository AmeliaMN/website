library(readr)
STAT360 <- read_csv("~/Downloads/2019-02-07T1254_Grades-Advanced_Statistical_Software.csv")
View(STAT360)
library(tidyverse)
STAT360 <- STAT360 %>%
filter(Student != "Points Possible")
