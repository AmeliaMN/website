install.packages(c("tidyverse", "Stat2Data", "skimr"))
library(tidyverse)
library(Stat2Data)
library(skimr)
data(FruitFlies)
head(FruitFlies)
FruitFlies %>%
skim(Treatment)
FruitFlies %>%
count(Treatment)
# Set the reference level
FruitFlies <- FruitFlies %>%
mutate(Treatment = relevel(Treatment, ref="none"))
FruitFlies %>%
count(Treatment)
ggplot(data=FruitFlies) + geom_dotplot(aes(x=Longevity))
ggplot(data=FruitFlies) + geom_dotplot(aes(x=Longevity))
?geom_dotplot
ggplot(data=FruitFlies) + geom_dotplot(aes(x=Longevity), binwidth=1)
ggplot(mtcars, aes(x = mpg)) + geom_dotplot()
dev.off()
ggplot(mtcars, aes(x = mpg)) + geom_dotplot()
ggplot(data=FruitFlies) + geom_boxplot(aes(x=Longevity), binwidth=1)
ggplot(data=FruitFlies) + geom_dotplot(aes(x=Longevity), binwidth=1)
ggplot(data=FruitFlies) + geom_boxplot(aes(x=Longevity, y=Treatment), binwidth=1)
ggplot(data=FruitFlies) + geom_boxplot(aes(x=Longevity, y=Treatment))
FruitFlies %>%
group_by(Treatment) %>%
skim(Longevity)
fm_null <- lm(Longevity ~ 1, data=FruitFlies)
summary(fm_null)
FruitFlies %>%
skim(Longevity)
fitted <- fitted.values(fm_null)
ggplot(data=FruitFlies) + geom_dotplot(aes(x=Longevity)) + geom_vline(xintercept = coef(fm_null))
FruitFlies %>%
group_by(Treatment) %>%
summarize(groupmean = mean(Longevity)) %>%
ungroup %>%
mean(Longevity~Treatment, data = FruitFlies) - mean(~Longevity, data = FruitFlies)
ggplot(data=FruitFlies) + geom_dotplot(aes(x=Longevity)) + geom_vline(xintercept = mean(Longevity)) + facet_wrap(~Treatment)
ggplot(data=FruitFlies) + geom_dotplot(aes(x=Longevity)) + geom_vline(xintercept = effects) + facet_wrap(~Treatment)
